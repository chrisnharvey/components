{
    "name": "encorephp/kernel",
    "description": "The EncorePHP Kernel.",
    "keywords": ["framework", "kernel", "encorephp"],
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Harvey",
            "email": "chris@chrisnharvey.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "ircmaxell/password-compat": "1.0.*",
        "monolog/monolog": "1.*",
        "nesbot/carbon": "1.*",
        "patchwork/utf8": "1.1.*",
        "symfony/finder": "2.3.*",
        "symfony/debug": "2.3.*",
        "illuminate/config": "4.0.*",
        "illuminate/events": "4.0.*",
        "illuminate/translation": "4.0.*",
        "illuminate/encryption": "4.0.*",
        "illuminate/database": "4.0.*",
        "illuminate/filesystem": "4.0.*"
    },
    "require-dev": {
        "encorephp/development": "dev-master"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/encorephp/development"
        }
    ],
    "autoload": {
        "psr-0": {
            "": "src/"
        },
        "files": [
            "src/Encore/Foundation/helpers.php"
        ]
    },
    "minimum-stability": "dev"
}
