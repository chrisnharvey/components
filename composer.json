        "jeremeamia/superclosure": "1.0.*",
        "monolog/monolog": "1.*",
        "nesbot/carbon": "1.*",
        "patchwork/utf8": "1.1.*",
        "symfony/finder": "2.3.*",
        "symfony/process": "2.3.*",
