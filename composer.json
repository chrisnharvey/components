{
    "name": "encorephp/components",
    "description": "EncorePHP super-project of components",
    "keywords": ["encorephp"],
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Harvey",
            "email": "chris@chrisnharvey.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "symfony/debug": "2.6.*",
        "symfony/finder": "2.6.*",
        "symfony/translation": "2.6.*",
        "symfony/yaml": "2.6.*",
        "sabre/event": "dev-master",
        "sabre/xml": "dev-master",
        "ircmaxell/password-compat": "1.0.*",
        "monolog/monolog": "1.*",
        "guzzlehttp/guzzle": "4.0.0-rc.1",
        "composer/composer": "1.0.*",
        "symfony/process": "2.6.*",
        "symfony/console": "2.6.*",
        "mockery/mockery": "0.8.0",
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-4": {
            "Encore\\Compiler\\": "src/compiler/src",
            "Encore\\Config\\": "src/config/src",
            "Encore\\Console\\": "src/console/src",
            "Encore\\Container\\": "src/container/src",
            "Encore\\Database\\": "src/database/src",
            "Encore\\Debug\\": "src/debug/src",
            "Encore\\Development\\": "src/development/src",
            "Encore\\Error\\": "src/error/src",
            "Encore\\Events\\": "src/events/src",
            "Encore\\Filesystem\\": "src/filesystem/src",
            "Encore\\Giml\\": "src/giml/src",
            "Encore\\": "src/kernel/src",
            "Encore\\Migration\\": "src/migration/src",
            "Encore\\Namespacer\\": "src/namespacer/src",
            "Encore\\Orm\\": "src/orm/src",
            "Encore\\Repl\\": "src/repl/src",
            "Encore\\Resource\\": "src/resource/src",
            "Encore\\Testing\\": "src/testing/src",
            "Encore\\Translation\\": "src/translation/src",
            "Encore\\View\\": "src/view-controller/src/View",
            "Encore\\Controller\\": "src/view-controller/src/Controller",
            "Encore\\Wx\\": "src/wxwidgets/src"
        }
    },
    "bin": ["src/development/bin/encore"],
    "minimum-stability": "dev"
}
