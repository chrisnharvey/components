        "encorephp/container": "dev-master",
        "encorephp/config": "dev-master",
        "encorephp/events": "dev-master",
        "encorephp/resource": "dev-master",
        "encorephp/view": "dev-master",
        "ircmaxell/password-compat": "1.0.*",
        "monolog/monolog": "1.*",
        "nesbot/carbon": "1.*",
        "patchwork/utf8": "1.1.*",
        "symfony/debug": "2.3.*"
    },
    "require-dev": {
