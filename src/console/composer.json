{
    "name": "encorephp/console",
    "description": "Simplified version of Symfony Console Component",
    "keywords": ["console"],
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Harvey",
            "email": "chris@chrisnharvey.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "symfony/console": "2.6.*"
    },
    "require-dev": {
        "encorephp/error": "dev-master",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "4.3.*"
    },
    "suggest": {
        "encorephp/error": "Allows the use of consoles error and exception displayer"
    },
    "autoload": {
        "psr-4": {
            "Encore\\Console\\": "src/"
        }
    },
    "minimum-stability": "dev"
}
