{
    "name": "encorephp/translation",
    "description": "Translation package based on Symfony Translation.",
    "keywords": ["translation", "translator", "lauguage"],
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Harvey",
            "email": "chris@chrisnharvey.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "encorephp/resource": "dev-master",
        "symfony/translation": "2.6.*"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "4.3.*"
    },
    "autoload": {
        "psr-4": {
            "Encore\\Translation\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    },
    "minimum-stability": "dev"
}